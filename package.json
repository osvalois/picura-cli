{
    "name": "picura",
    "version": "1.0.0",
    "type": "module",
    "description": "PICURA: CLI Avanzada para GestiÃ³n Integral de Proyectos de Software",
    "author": "Oscar Valois",
    "bin": {
        "picura": "./bin/run.js"
    },
    "homepage": "https://github.com/osvalois/picura-cli",
    "license": "MIT",
    "main": "dist/index.js",
    "repository": "osvalois/picura-cli",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@babel/parser": "^7.22.7",
        "@babel/traverse": "^7.22.8",
        "@huggingface/inference": "^2.6.1",
        "@oclif/core": "^4.0.21",
        "@oclif/plugin-help": "^5.2.14",
        "@oclif/plugin-plugins": "^3.1.8",
        "@prisma/client": "^5.19.1",
        "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
        "@tensorflow/tfjs-node": "^4.21.0",
        "@types/inquirer": "^9.0.7",
        "axios": "^1.7.7",
        "boxen": "^7.1.1",
        "chalk": "^5.3.0",
        "dotenv": "^16.3.1",
        "figlet": "^1.6.0",
        "fs-extra": "^11.1.1",
        "glob": "^10.3.3",
        "gpt-3-encoder": "^1.1.4",
        "inquirer": "^9.2.10",
        "java-parser": "^2.0.5",
        "natural": "^6.5.0",
        "openai": "^3.3.0",
        "ora": "^7.0.1",
        "php-parser": "^3.1.5",
        "recast": "^0.23.4",
        "simple-git": "^3.19.1",
        "tree-sitter": "^0.21.1",
        "tree-sitter-c-sharp": "^0.23.0",
        "tree-sitter-cpp": "^0.20.3",
        "tree-sitter-go": "^0.20.0",
        "tree-sitter-java": "^0.20.2",
        "tree-sitter-javascript": "^0.23.0",
        "tree-sitter-php": "^0.19.0",
        "tree-sitter-python": "^0.20.4",
        "tree-sitter-ruby": "^0.19.0",
        "tree-sitter-rust": "^0.20.4",
        "tree-sitter-typescript": "^0.20.3",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@c/test": "^2.4.4",
        "@types/babel__traverse": "^7.20.1",
        "@types/figlet": "^1.5.6",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.4.5",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "eslint": "^8.45.0",
        "eslint-config-oclif": "^4.0.0",
        "eslint-config-oclif-typescript": "^1.0.3",
        "globby": "^13.2.2",
        "oclif": "^3.11.3",
        "prisma": "^5.19.1",
        "shx": "^0.3.4",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.1",
        "typescript": "^5.1.6"
    },
    "oclif": {
        "bin": "picura",
        "dirname": "picura",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "init": {
                "description": "Initialize a new PICURA project"
            },
            "generate": {
                "description": "Generate project documentation"
            }
        },
        "hooks": {
            "init": "./dist/hooks/init.js"
        }
    },
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "npm run lint",
        "prepack": "npm run build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "start": "node --experimental-specifier-resolution=node bin/run.js",
        "dev": "node --loader ts-node/esm src/index.ts",
        "unlink": "npm unlink -g picura-cli",
        "link": "npm link",
        "update-local": "npm run unlink && npm install && npm run build && npm run link"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "bugs": "https://github.com/osvalois/picura-cli/issues",
    "keywords": [
        "oclif",
        "cli",
        "project-management",
        "documentation",
        "ai"
    ],
    "types": "dist/index.d.ts"
}
